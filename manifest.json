{
    "manifest_version": 3,
    "name": "HTTP Warning Extension",
    "version": "1.0",
    "description": "Warns you if the site is HTTP and ignores HTTPS sites.",
    "permissions": [
      "tabs",
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  